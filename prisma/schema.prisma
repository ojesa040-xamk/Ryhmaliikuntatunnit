// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Tunnit {
  TunnitId Int @id @default(autoincrement())
  tunti String
  paivanmaara String
  kellonaika String?
  osallistujat  Kayttajat[]  @relation("Ilmoittautumiset")
}

model Kayttajat {
  kayttajaId Int @id @default(autoincrement())
  kayttajatunnus String
  salasana String
  ilmoittautumiset Tunnit[]  @relation("Ilmoittautumiset")
}